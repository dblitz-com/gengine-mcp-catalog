# Environment variables for local Act testing
# Copy this to .env.local and fill in actual values

# Docker configuration
DOCKER_BUILDKIT=1
COMPOSE_DOCKER_CLI_BUILD=1

# Registry configuration  
REGISTRY=ghcr.io
IMAGE_NAME=devin/dblitz/gengine-rest-api

# AWS region
AWS_REGION=us-west-2

# Local testing flags
RUN_LOCAL=true
ACT_LOCAL=true

# Skip actual deployments in local testing
SKIP_DEPLOYMENT=true